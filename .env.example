# Environment: local or prod
ENVIRONMENT=local

# Gemini API Settings
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_GEN_MODEL=gemini-2.0-flash

# LiteLLM Settings
LITELLM_API_KEY=your_litellm_api_key_here

# Qdrant Database Settings (Local)
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Qdrant Database Settings (Prod)
QDRANT_URL=your_qdrant_cloud_url
QDRANT_API_KEY=your_qdrant_api_key

# API Settings
PROJECT_NAME="Embeddings Optimization API"
VERSION=1.0.0

# Ngrok Configuration (for production mode with public tunnels)
NGROK_AUTH_TOKEN=your_ngrok_token_here
