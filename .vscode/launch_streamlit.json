{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Streamlit",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "streamlit_app/app.py",
        "--server.port",
        "8501",
        "--server.address",
        "0.0.0.0"
      ],
      "jinja": true,
      "justMyCode": true,
      "python": "${workspaceFolder}/.venv/Scripts/python.exe"
    }
  ]
}
